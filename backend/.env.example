# Rename this file to .env and fill in the values
MONGO_URI=your_mongodb_connection_string
JWT_SECRET=your_super_secret_jwt_key
PORT=5001

# Admin seed (optional; used when running seed script if no admin exists)
ADMIN_EMAIL=admin@crea.com
ADMIN_PASSWORD=12345678
ADMIN_FORCE_RESET=false

# SMTP settings for email OTP (use Gmail App Passwords)
# Note: Do NOT wrap values in quotes. For App Password, enter the 16-character value without spaces.
SMTP_HOST=smtp.gmail.com
SMTP_PORT=465
SMTP_USER=your_gmail_address@gmail.com
SMTP_PASS=your_gmail_app_password
SMTP_FROM=CREA <your_gmail_address@gmail.com>


